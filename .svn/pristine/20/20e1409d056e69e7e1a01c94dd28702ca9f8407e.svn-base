package Kinect;
import SimpleOpenNI.SimpleOpenNI;
import processing.core.PApplet;

public class EnableScene implements EnableStrategy{

	@Override
	public SimpleOpenNI enable(PApplet p) {
		SimpleOpenNI context = new SimpleOpenNI(p);
		if(context.enableScene()==false) {
			System.out.println("Fehler beim initialisieren der Kinect");
		}
		return context;
	}

}